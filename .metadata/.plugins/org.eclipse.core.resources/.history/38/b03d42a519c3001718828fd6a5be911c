import java.util.*;
public class Vectoresej6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double v[][] = new double [10][10];
		int fila, col, sumaFila, sumaCol;
		for( fila = 0; fila < 10; fila ++){
			for (col = 0; col < 10; col ++){
			v[fila][col] = (int)Math.floor(Math.random() * 11 -5);
			}
		}//for
		for (fila = 0; fila < 10; fila++) {
			sumaFila=0;
			for (col = 0; col < 10; col ++){
				sumaFila = sumaFila + (int)v[fila][col];
				System.out.print(v[fila][col] );
			}//for
			
		}//for
		for (col = 0; col < 10; col ++) {
			sumaCol=0;
			for( fila = 0; fila < 10; fila ++){
				sumaCol = sumaCol + (int)v[fila][col];
			}//for
		}//for
		for( fila = 0; fila < 10; fila ++){
			
			for (col = 0; col < 10; col ++){
				System.out.println( v[fila][col] );
			}	
		}
	}//main

}//class
